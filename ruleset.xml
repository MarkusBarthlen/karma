<?xml version="1.0"?>

<ruleset name="PMD-Rules">
  <description>
	The rulesets we want to use in pmd. For a detailed description of pmd rulesets: http://pmd.sourceforge.net/rules/index.html
  </description>
  
  
  

  <rule ref="rulesets/java/codesize.xml/CyclomaticComplexity">					<priority>5</priority></rule>
  
  <rule ref="rulesets/java/controversial.xml/AvoidLiteralsInIfCondition">					<priority>5</priority></rule>
  <rule ref="rulesets/java/controversial.xml/OnlyOneReturn">					<priority>5</priority></rule>
  
  <rule ref="rulesets/java/strings.xml/UseEqualsToCompareStrings"/>
   
  <rule ref="rulesets/java/logging-java.xml/SystemPrintln"/>

  <rule ref="rulesets/java/javabeans.xml/BeanMembersShouldSerialize"/>

  <rule ref="rulesets/java/javabeans.xml/MissingSerialVersionUID"/>
  


  </ruleset>